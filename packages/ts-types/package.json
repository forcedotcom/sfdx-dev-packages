{
  "name": "@salesforce/ts-types",
  "version": "0.17.1",
  "description": "Types and related utilities for TypeScript",
  "main": "lib/index.js",
  "repository": "https://git.soma.salesforce.com/salesforcedx/sfdx-ts-types",
  "author": "Salesforce",
  "license": "BSD-3-Clause",
  "types": "lib/index.d.ts",
  "files": [
    "docs",
    "lib/**/*.js",
    "!lib/**/*.test.js",
    "lib/**/*.d.ts",
    "!lib/**/*.test.d.ts"
  ],
  "scripts": {
    "build": "yarn sfdx-build",
    "clean": "yarn sfdx-clean",
    "clean-all": "yarn sfdx-clean all",
    "compile": "yarn sfdx-compile",
    "docs": "yarn sfdx-docs",
    "format": "yarn sfdx-format",
    "lint": "yarn sfdx-lint",
    "postcompile": "shx cp -r src/types lib",
    "prepare": "yarn build && yarn docs",
    "test": "yarn sfdx-test"
  },
  "dependencies": {
    "tslib": "^1"
  },
  "devDependencies": {
    "@salesforce/dev-config": "^1.3.1",
    "@salesforce/dev-scripts": "^0.2.1"
  }
}
