{
  "name": "@salesforce/dev-scripts",
  "version": "0.9.0",
  "description": "Standardize package.json scripts and config files for Salesforce projects.",
  "repository": "https://github.com/forcedotcom/sfdx-dev-packages",
  "bin": {
    "sf-build": "bin/sf-build.js",
    "sf-ci-docs": "bin/sf-ci-docs.js",
    "sf-clean": "bin/sf-clean.js",
    "sf-compile": "bin/sf-compile.js",
    "sf-compile-test": "bin/sf-compile-test.js",
    "sf-docs": "bin/sf-docs.js",
    "sf-format": "bin/sf-format.js",
    "sf-husky-commit-msg": "bin/sf-husky-commit-msg.js",
    "sf-husky-pre-commit": "bin/sf-husky-pre-commit.js",
    "sf-husky-pre-push": "bin/sf-husky-pre-push.js",
    "sf-install": "bin/sf-install.js",
    "sf-lerna-install": "bin/sf-lerna-install.js",
    "sf-lint": "bin/sf-lint.js",
    "sf-test": "bin/sf-test.js"
  },
  "scripts": {
    "format": "prettier --write \"**/*.+(js|json)\"",
