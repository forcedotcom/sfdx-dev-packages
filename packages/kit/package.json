{
  "name": "@salesforce/kit",
  "version": "0.10.0",
  "description": "Commonly needed utilities for TypeScript and JavaScript",
  "main": "lib/index.js",
  "repository": "https://git.soma.salesforce.com/salesforcedx/sfdx-kit",
  "author": "Salesforce",
  "license": "BSD-3-Clause",
  "types": "lib/index.d.ts",
  "files": [
    "docs",
    "lib/**/*.js",
    "!lib/**/*.test.js",
    "lib/**/*.d.ts",
    "!lib/**/*.test.d.ts",
    "vendor/lodash.js"
  ],
  "scripts": {
    "build": "yarn compile && yarn lint",
    "clean": "yarn sfdx-clean",
    "clean-all": "yarn sfdx-clean all",
    "compile": "yarn sfdx-compile",
    "docs": "yarn sfdx-docs",
    "format": "yarn sfdx-format",
    "lint": "yarn sfdx-lint",
    "lodash": "./scripts/build-lodash.sh",
    "prepare": "yarn build && yarn docs",
    "test": "yarn sfdx-test"
  },
  "dependencies": {
    "@salesforce/ts-types": "^0.16.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@salesforce/dev-config": "^1.2.0",
    "@salesforce/dev-scripts": "^0.1.0",
    "lodash-cli": "^4"
  }
}
