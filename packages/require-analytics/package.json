{
  "name": "@salesforce/require-analytics",
  "version": "0.5.0",
  "description": "Simple CommonJS require analytics",
  "main": "lib/index.js",
  "repository": "https://git.soma.salesforce.com/salesforcedx/sfdx-require-analytics",
  "author": "Salesforce",
  "license": "BSD-3-Clause",
  "types": "lib/index.d.ts",
  "files": [
    "docs",
    "lib/**/*.js",
    "!lib/**/*.test.js",
    "lib/**/*.d.ts",
    "!lib/**/*.test.d.ts"
  ],
  "scripts": {
    "build": "yarn sfdx-build",
    "clean": "yarn sfdx-clean",
    "clean-all": "yarn sfdx-clean all",
    "compile": "yarn sfdx-compile",
    "docs": "yarn sfdx-docs",
    "format": "yarn sfdx-format",
    "lint": "yarn sfdx-lint",
    "prepack": "yarn sfdx-build",
    "prepare": "yarn build && yarn docs",
    "rebuild": "yarn clean-all && yarn install --no-lockfile && yarn build",
    "test": "yarn sfdx-test",
    "uts": "nyc mocha --require source-map-support/register --recursive \"lib/**/*.test.js\"",
    "watch": "tsc -w -p tsconfig.json"
  },
  "devDependencies": {
    "@salesforce/dev-scripts": "^0.3.3"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 60,
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "lib/**/*.test.js"
    ]
  },
  "dependencies": {
    "tslib": "^1.9.3"
  }
}
