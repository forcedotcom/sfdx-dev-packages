{
  "name": "@salesforce/ts-types",
  "version": "0.16.0",
  "description": "Types and related utilities for TypeScript",
  "main": "lib/index.js",
  "repository": "https://git.soma.salesforce.com/salesforcedx/sfdx-ts-types",
  "author": "Salesforce",
  "license": "BSD-3-Clause",
  "types": "lib/index.d.ts",
  "files": [
    "docs",
    "lib/**/*.js",
    "!lib/**/*.test.js",
    "lib/**/*.d.ts",
    "!lib/**/*.test.d.ts"
  ],
  "scripts": {
    "build": "yarn compile && yarn lint",
    "clean": "yarn sfdx-clean",
    "clean-all": "yarn sfdx-clean all",
    "compile": "yarn sfdx-compile",
    "docs": "rm -rf docs/* && typedoc --out docs",
    "lint": "yarn sfdx-lint",
    "postcompile": "cp -r src/types lib",
    "prepare": "yarn build && yarn docs",
    "rebuild": "yarn clean-all && yarn install --no-lockfile && yarn build",
    "test": "yarn sfdx-test",
    "uts": "nyc mocha --require source-map-support/register --recursive \"lib/**/*.test.js\"",
    "watch": "tsc -w -p tsconfig.json"
  },
  "dependencies": {
    "tslib": "^1"
  },
  "devDependencies": {
    "@salesforce/dev-config": "^1.2.0",
    "@salesforce/dev-scripts": "^0.1.0"
  }
}
