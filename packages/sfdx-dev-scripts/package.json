{
  "name": "@salesforce/dev-scripts",
  "version": "0.0.0",
  "description": "A set of standard package.json scripts for Salesforce projects.",
  "bin": {
    "sfdx-clean": "bin/sfdx-clean.js",
    "sfdx-compile": "bin/sfdx-compile.js"
  },
  "scripts": {
    "clean": "bin/sfdx-clean.js",
    "clean-all": "bin/sfdx-clean.js all",
    "postinstall": "bin/sfdx-gen-scripts-post.js",
    "preinstall": "bin/sfdx-gen-scripts-pre.js"
  },
  "keywords": [
    "sfdx"
  ],
  "author": "Platform CLI",
  "license": "BSD-3-Clause",
  "files": [
    "/bin/"
  ],
  "dependencies": {
    "@commitlint/cli": "^7",
    "@commitlint/config-conventional": "^7",
    "shelljs": "0.8.2",
    "typescript": "^3"
  },
  "devDependencies": {
    "husky": "^1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "bin/sfdx-commit-msg.js"
    }
  }
}