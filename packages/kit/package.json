{
  "name": "@salesforce/kit",
  "version": "0.12.1",
  "description": "Commonly needed utilities for TypeScript and JavaScript",
  "main": "lib/index.js",
  "repository": "https://git.soma.salesforce.com/salesforcedx/sfdx-kit",
  "author": "Salesforce",
  "license": "BSD-3-Clause",
  "types": "lib/index.d.ts",
  "files": [
    "docs",
    "lib/**/*.js",
    "!lib/**/*.test.js",
    "lib/**/*.d.ts",
    "!lib/**/*.test.d.ts",
    "vendor/lodash.js"
  ],
  "scripts": {
    "build": "yarn sfdx-build",
    "clean": "yarn sfdx-clean",
    "clean-all": "yarn sfdx-clean all",
    "compile": "yarn sfdx-compile",
    "docs": "yarn sfdx-docs",
    "format": "yarn sfdx-format",
    "lint": "yarn sfdx-lint",
    "lodash": "./scripts/build-lodash.sh",
    "postcompile": "shx cp -r src/nodash/support.d.ts lib/nodash",
    "prepare": "yarn build && yarn docs",
    "test": "yarn sfdx-test"
  },
  "dependencies": {
    "@salesforce/ts-types": "^0.17.4",
    "tslib": "^1"
  },
  "devDependencies": {
    "@salesforce/dev-config": "^1.4.0",
    "@salesforce/dev-scripts": "^0.3.3",
    "lodash-cli": "^4"
  }
}
