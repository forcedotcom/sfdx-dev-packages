    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.6",
    "@types/sinon": "^9.0.4",
    "chai": "^4.2.0",
    "chalk": "^4.0.0",
    "cosmiconfig": "^6.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
