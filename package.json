{
  "name": "sfdx-dev-packages",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "ci-docs": "yarn sf-ci-docs",
    "clean": "lerna run clean",
    "clean-all": "lerna run clean-all",
    "codecov": "codecov --disable=gcov",
    "compile": "lerna run compile",
    "docs": "lerna run docs",
    "format": "lerna run format",
    "install": "./packages/dev-scripts/bin/sf-lerna-install.js",
    "lint": "lerna run lint",
    "lint-fix": "lerna run lint -- --fix",
    "postclean-all": "yarn sf-clean all",
    "postinstall": "yarn bootstrap",
    "prepack": "lerna run prepack",
    "test": "lerna run test"
  },
  "devDependencies": {
    "@salesforce/dev-config": "^2.0.0",
    "@salesforce/prettier-config": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "chai": "^4.2.0",
    "codecov": "^3.7.1",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-salesforce": "^0.1.0",
    "eslint-config-salesforce-license": "^0.1.0",
    "eslint-config-salesforce-typescript": "^0.1.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^27.0.3",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lerna": "^3.22.1",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "sinon": "^9.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "workspaces": [
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "sf-husky-commit-msg",
      "pre-commit": "sf-husky-pre-commit",
      "pre-push": "sf-husky-pre-push"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/forcedotcom/sfdx-dev-packages.git"
  },
  "license": "BSD-3-Clause"
}
